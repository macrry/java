package demo;

import java.util.Scanner;

public class de05 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc =new Scanner(System.in);
		System.out.println("请输入两个整数：");
		int a=sc.nextInt();
		int b=sc.nextInt();
		System.out.println(zida(a, b));
	}
	public static int zida(int x,int y) {
		int num=0;
		for (int i = x; i >=1; i--) {
			if (x%i==0 && y%i==0) {
				return i;
			}
		}
		return 1;
	}
}
