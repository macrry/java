import java.util.Scanner;
class de09 
{
	public static void main(String[] args) 
	{
		Scanner sc =new Scanner(System.in);
		System.out.println("请输入一个1~7之间的数字");
		int day = sc.nextInt();

		String weekday = "";
		switch (day)
		{
		case 1 :
		weekday ="星期一";
			break;
		case 2 :
		weekday ="星期二";
			break;
		case 3 :
		weekday ="星期三";
			break;
		case 4 :
		weekday ="星期四";
			break;
		case 5 :
		weekday ="星期五";
			break;
		case 6 :
		weekday ="星期六";
			break;
		case 7 :
		weekday ="星期日";
			break;
		default:
			weekday = "您输入的数字有错";
		}
		System.out.println(weekday);
	}
}
